// schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model ImageMetadata {
  id        String     @default(cuid()) @id
  userId    String
  imageUrl  String
  imageDownloadUrl String
  imageWeekNumber  Int
  uploadDate BigInt
}
